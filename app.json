{
  "expo": {
    "name": "boost-me",
    "slug": "boost-me",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app_icon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app_icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.devyasirmehboob.boostme"
    },
    "web": {
      "favicon": "./src/assets/app_icon.jpg"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./src/assets/splash-icon.png",
          "imageWidth": 200
        }
      ],
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "78dc2cfa-8d8a-458c-ae6d-4a4b58dc2f53"
      }
    }
  }
}